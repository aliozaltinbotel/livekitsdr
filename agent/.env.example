# LiveKit Cloud credentials (used by both frontend and agent)
LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# AI service credentials (used by agent)
OPENAI_API_KEY=your-openai-api-key
DEEPGRAM_API_KEY=your-deepgram-api-key
CARTESIA_API_KEY=your-cartesia-api-key
ASSEMBLYAI_API_KEY=your-assemblyai-api-key

# Google Calendar configuration
# For Service Account: Path to service account key file
GOOGLE_SERVICE_ACCOUNT_KEY=/app/botelai-service-account.json
# Calendar ID (use "primary" for the user's primary calendar)
GOOGLE_CALENDAR_ID=primary
# For Domain-Wide Delegation: Email of user to impersonate
GOOGLE_DELEGATED_USER_EMAIL=user@example.com

# Optional: Supabase credentials for conversation logging
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key

# PMS MCP Server configuration
PMS_MCP_SERVER_URL=http://localhost:3001/sse
PMS_MCP_SERVER_TOKEN=your-mcp-server-token

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO