# LiveKit Cloud credentials
LIVEKIT_URL=wss://<your-project>.livekit.cloud
LIVEKIT_API_KEY=<your-api-key>
LIVEKIT_API_SECRET=<your-api-secret>

# AI service credentials
OPENAI_API_KEY=sk-...
DEEPGRAM_API_KEY=...
CARTESIA_API_KEY=...

# Optional: Google Calendar integration
# For OAuth2: Path to credentials.json file downloaded from Google Cloud Console
GOOGLE_CREDENTIALS_PATH=/path/to/credentials.json
# For Service Account: Path to service account key file
GOOGLE_SERVICE_ACCOUNT_KEY=/path/to/service-account-key.json
# Calendar ID (use "primary" for the user's primary calendar)
GOOGLE_CALENDAR_ID=primary
# For Domain-Wide Delegation: Email of user to impersonate
GOOGLE_DELEGATED_USER_EMAIL=your-email@botel.ai

# Optional: Supabase credentials for conversation logging
SUPABASE_URL=https://<your-project>.supabase.co
SUPABASE_KEY=<your-anon-key>